{
    "name": "smolevich/reindexer-client",
    "description": "PHP client for using database reindexer",
    "type": "library",
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "friendsofphp/php-cs-fixer": "^2.14",
        "php-coveralls/php-coveralls": "dev-add-support-for-github-actions@dev"
    },
    "autoload": {
        "psr-4": {
            "": "src/",
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
          "type": "vcs",
          "url": "https://github.com/Smolevich/php-coveralls"
        }
    ],
    "authors": [
        {
            "name": "Stanislav Shupilkin",
            "email": "smolevich90@gmail.com"
        }
    ],
    "scripts": {
        "run-test": "phpunit",
        "style-fix": "php-cs-fixer fix"
    }
}
